        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|^6|^7|^8|^9",
        "nesbot/carbon": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "GetCode\\LaravelLogs\\": "src"
